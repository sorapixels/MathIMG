{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [{
      "source": "*.@(svg|png)",
      "headers": [{
        "key": "Cache-Control",
        "value": "public, max-age=31557600, s-maxage=31557600"
      }]
    }],
    "rewrites": [{
      "source": "**/*.@(svg|png)",
      "function": "image"
    }, {
      "source": "**",
      "destination": "/index.html"
    }],
    "cleanUrls": true,
    "trailingSlash": false
  }
}